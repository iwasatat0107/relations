<div class="contents row">
  <div class="container">
    <h3>作成</h3>
    <%= form_with(model: @client, local: true) do |form| %>
      <%= form.text_field :last_name, placeholder: "last_name" %>
      <%= form.text_field :first_name, placeholder: "first_name" %>
      <%= form.text_field :last_name_kana, placeholder: "last_name_kana" %>
      <%= form.text_field :first_name_kana, placeholder: "first_name_kana" %>
      <%= form.text_field :company, placeholder: "company" %>
      <%= form.text_field :prefecture_id, placeholder: "prefecture_id" %>
      <%= form.text_field :birthday, placeholder: "birthday" %>
      <%= form.text_field :image, placeholder: "Image Url" %>
      <%= form.submit "SEND" %>
    <% end %>
  </div>
</div>